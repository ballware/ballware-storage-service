{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {    
    "Limits": {
      "MaxRequestBodySize": 40000000
    }
  },
  "Authorization": {
    "Authority": "fake_authority",
    "Audience": "fake_audience",
    "RequiredScopes": "openid fakescope"
  },
  "Storage": {
    "ConnectionString": "fake_connection_string",
    "Share": "fake_share"
  },
  "Swagger": {
    "EnableClient": true,
    "ClientId": "fake_client"
  },
  "Cors": {
    "AllowedOrigins": "*",
    "AllowedMethods": "*",
    "AllowedHeaders": "*"
  }
}
