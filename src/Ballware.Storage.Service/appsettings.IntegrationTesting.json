{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {    
    "Limits": {
      "MaxRequestBodySize": 40000000
    }
  },
  "Authorization": {
    "Authority": "https://localhost:29999",
    "Audience": "fake_audience",
    "RequiredScopes": "storageApi"
  },
  "Meta": {
    "AutoMigrations": true
  },
  "Swagger": {
    "EnableClient": true,
    "ClientId": "fake_client"
  },
  "Cors": {
    "AllowedOrigins": "*",
    "AllowedMethods": "*",
    "AllowedHeaders": "*"
  }
}
